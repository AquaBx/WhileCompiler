function getHead: 
read 
% 
    List := (cons (cons a b) (cons c d));
    Result := (hd List)
% 
write Result

function getTail: 
read 
% 
    List := (cons (cons a b) (cons c d));
    Result := (tl List)
% 
write Result

function getHeadNil: 
read 
% 
    List := nil;
    Result := (hd List)
% 
write Result

function getTailNil: 
read 
% 
    List := nil;
    Result := (tl List)
% 
write Result